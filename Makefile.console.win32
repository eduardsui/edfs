CC = gcc
RM = rm
CFLAGS = -O3 -D_FILE_OFFSET_BITS=64
LIBS = -lws2_32 -lwinmm
BUILDFLAGS= -o edfs_mount

SRC = sha256.c xxhash.c base64.c parson.c edd25519.c avl.c chacha.c log.c sha3.c edwork.c edfs_core.c edfs_console.c
OBJS = $(SRC: .c=.o)

edfs: ${OBJS}
	${CC} ${BUILDFLAGS} ${CFLAGS} ${OBJS} ${LIBS}

%.o:
	${CC} ${CFLAGS} -c $<

.PHONY: clean
clean:
	@echo all cleaned up!
